<div class="map-select placeholder-styling" >
    <h2>Select Map:</h2>
    <ul id="map-list">
        {{#each maps}}
        
            <li><button>{{name}}</button></li>

        {{/each}}
    </ul>
    <h2>Alternatively, Create a New Map</h2>
    <input id="map-width" type="number" placeholder="Width" min="1" max="512">
    <input id="map-height" type="number" placeholder="Height" min="1" max="512">
    <button id="create-map-button">Create</button>
</div>